{
    "hosting": {
        "public": "/",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**",
            "package.json",
            "package-lock.json",
            "README.md"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}
